<!-- Main jumbotron for a primary marketing message or call to action -->
<div class="jumbotron">
  <div class="container">
    <h1 id="jumbotronText">Learn</h1>
  </div>
</div>
<div class="row">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Balanced Fund - Holdings</h3>
      </div>
      <div class="panel-body">
        <% include ../template/flash.ejs %>
          <div class="table-responsive">
          <table class="table table-striped" >
            <caption>Unit Detail</caption>
            <thead>
              <tr>
                <th>Currency</th>
                <th>Value</th>
              </tr>
            </thead>
            <tbody>
                <% for(i=0;i<unitPrice.currencies.length; i++) { %>
                <tr>
                  <td><%= unitPrice.currencies[i].currencyName %></td>
                  <td><%= unitPrice.currencies[i].currencyValue %></td>
                </tr>
                <% } %>     
            </tbody>
          </table>
        </div>
    </div>
  </div>
</div><!-- /.row -->
</div><!-- /.container -->